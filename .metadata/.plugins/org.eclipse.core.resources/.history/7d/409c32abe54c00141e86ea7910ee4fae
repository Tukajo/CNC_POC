package com.nanospark.cnc;

import java.util.ArrayList;
import java.util.Calendar;

import android.text.format.Time;

public class NotificationEvent extends Event {

	public NotificationEvent( int assignedPin, String trackingType,
			Calendar startDate, Calendar endDate, boolean watchAllDay,
			LocalTime startTime, LocalTime endTime, ArrayList<String> selectedDays,
			ContactInfo selectedContact, String formatChoice,
			ReportTimeSpec reportTimingConfig) {
		super(assignedPin, trackingType, startDate, endDate, watchAllDay,
				startTime, endTime, selectedDays, selectedContact, formatChoice,
				reportTimingConfig);
		// TODO Auto-generated constructor stub
	}

}
