package com.nanospark.cnc;

import java.util.Calendar;

public class Event {
	String type;
	Pin assignedPin;
	String trackingType;
	Calendar startDate;
	
	public Event(String type, Pin assignedPin, String trackingType){
		this.type = type;
		this.assignedPin = assignedPin;
		this.trackingType = trackingType;
		
	}
	
	
}
