package com.nanospark.cnc;

import java.util.Calendar;

public class Event {
	String type;
	Pin assignedPin;
	String trackingType;
	Calendar startDate;
	Calendar endDate;
	Boolean watchAllDay
	
	public Event(String type, Pin assignedPin, String trackingType, Calendar startDate, Calendar endDate, boolean watchAllDay){
		this.type = type;
		this.assignedPin = assignedPin;
		this.trackingType = trackingType;
		this.startDate = startDate;
		this.endDate = endDate;
	}
	
	
}
